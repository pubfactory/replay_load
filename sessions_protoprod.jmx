          <BeanShellPreProcessor guiclass="TestBeanGUI" testclass="BeanShellPreProcessor" testname="BeanShellPreProcessor - Set Method and Session" enabled="true">
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <boolProp name="resetInterpreter">false</boolProp>
            <stringProp name="script">import SessionInfoMap;
import org.apache.jmeter.protocol.http.control .CookieManager;
import org.apache.jmeter.protocol.http.control.Cookie;

sampler.setMethod(vars.get(&quot;method&quot;));

String awsalbSessionId;
String originalSessionVal = vars.get(&quot;session&quot;);

// Check the session info map singleton to see if there is a session entry from the original csv file
// If so, we set back to the server the corresponding current jsession via Set-Cookie header
if(SessionInfoMap.containsKey(originalSessionVal)) {
    awsalbSessionId = SessionInfoMap.get(originalSessionVal);
    log.info(&quot;======= ORIGINAL SESSION ID ======= : &quot; + originalSessionVal);
    
    // Set the cookie with the cached jsessionid that maps to the original session id from the request log
   Cookie cookie = new Cookie(&quot;AWSALB&quot;,awsalbSessionId,&quot;dg.dgsg-web.qa.sites.pubfactory.com&quot;,&quot;/&quot;, false,-1);
	CookieManager manager = sampler.getCookieManager();
	log.info(&quot;Setting Cookie to use AWSALB &quot; + awsalbSessionId);
	log.info(&quot;Thread corresponds to original session: &quot; + originalSessionVal);
	manager.add(cookie);
	//manager.save(cookie);
	// The new mapping will be added after the request is sent in the post-processor
}
else {
	log.info(&quot;========= WE SHOULD USE THE INCOMING AWSALB session cookie AND HOPEFULLY SET IT IN MAP&quot;);
}
</stringProp>
          </BeanShellPreProcessor>
          <hashTree/>
          <BeanShellTimer guiclass="TestBeanGUI" testclass="BeanShellTimer" testname="BeanShell Timer - Set Wait" enabled="true">
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <boolProp name="resetInterpreter">false</boolProp>
            <stringProp name="script">long wait=0;

long ts = Long.parseLong(vars.get(&quot;unix_ts&quot;));
//log.info(&quot;Timestamp as long: &quot; + ts);

long FirstTS=Long.parseLong(vars.get(&quot;FirstTS&quot;)); // First timestamp
//log.info(&quot;First Timestamp from the BeanShell Timer --&gt; &quot; + FirstTS);

wait=(ts-FirstTS)-(System.currentTimeMillis() - Long.parseLong(props.getProperty(&quot;TESTSTART.MS&quot;)));

// DEBUG 
wait = 1000; //1000 ms
log.info(&quot;Time stamp from replay log = &quot; + ts + &quot; FirstTS = &quot; + FirstTS + &quot; Total Wait Time = &quot; + wait + &quot; milliseconds&quot;);

return wait;</stringProp>
          </BeanShellTimer>
