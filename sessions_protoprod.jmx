          <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="BeanShellPostProcessor - Add new session map entry" enabled="true">
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <boolProp name="resetInterpreter">false</boolProp>
            <stringProp name="script">import SessionInfoMap;

String originalSessionVal = vars.get(&quot;session&quot;);

// Check the session info map singleton to see if there is a session entry from the original csv file
// If not, we add the mapping 
if(!SessionInfoMap.containsKey(originalSessionVal)) {
	String newJsessionId = vars.get(&quot;jsessionid&quot;);
	SessionInfoMap.put(originalSessionVal,newJsessionId);
	log.info(&quot;------Setting New Mapping ------&gt; &quot; + newJsessionId + &quot; ==== MAPS TO : &quot; + originalSessionVal);
}
</stringProp>
          </BeanShellPostProcessor>
